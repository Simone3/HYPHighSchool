<%= render :partial => "layouts/all_to_all_links", object: @all_to_all_links %>

<div id="main" class="main_element">
	
	<h1>Program for <%= @course.name %></h1>

	<p>
		<% if(@course.program && @course.program!='') %>
			<%= @course.program.html_safe %>
		<% else %>
			<i>There's no program available</i>
		<% end %>
	</p>

</div>